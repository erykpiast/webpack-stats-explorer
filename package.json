{
  "name": "webpack-stats-explorer",
  "version": "1.13.0",
  "description": "Tool for exploring webpack stats and comparing them between builds",
  "repository": {
    "type": "git",
    "url": "git@github.com:erykpiast/webpack-stats-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/erykpiast/webpack-stats-explorer/issues"
  },
  "homepage": "https://github.com/erykpiast/webpack-stats-explorer",
  "main": "dist/main.js",
  "files": [
    "dist/index.html",
    "dist/main.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "precompile": "bsb -clean-world",
    "compile": "bsb -make-world",
    "prebuild": "run-s compile clean",
    "build": "NODE_ENV=production webpack",
    "start": "bsb -make-world -w",
    "pretest": "npm run compile",
    "test": "TEST=true jest",
    "webpack": "webpack-dev-server",
    "format": "find src -name '*.ml' -exec xargs ocamlformat -i -p janestreet {} \\;",
    "release": "standard-version",
    "playground:play": "./playground/play.sh",
    "playground:upgrade-deps": "./playground/upgrade-deps.sh",
    "prepublish": "npm run build"
  },
  "keywords": [
    "webpack",
    "analyzer",
    "bundle",
    "diff",
    "compare",
    "stats"
  ],
  "author": "Eryk Napiera≈Ça <eryk.piast@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "5.0.1",
    "@glennsl/rebase": "0.2.1",
    "@zpnester/bs-filereader": "0.4.0",
    "bs-css": "10.0.1",
    "bs-jsverify": "0.10.0",
    "bs-react-dropzone": "0.5.0",
    "diff": "4.0.1",
    "rationale": "0.2.0",
    "re-classnames": "4.0.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-dropzone": "10.1.10",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@glennsl/bs-jest": "0.4.9",
    "babel-jest": "24.9.0",
    "bs-enzyme": "0.6.1",
    "bs-platform": "5.2.1",
    "enzyme-adapter-react-16": "1.15.1",
    "gh-pages": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jsverify": "0.8.4",
    "npm-run-all": "4.1.5",
    "standard-version": "7.0.0",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.9.0"
  }
}
