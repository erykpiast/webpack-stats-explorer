{
  "name": "webpack-stats-explorer",
  "version": "1.25.0",
  "description": "Tool for exploring webpack stats and comparing them between builds",
  "repository": {
    "type": "git",
    "url": "git@github.com:erykpiast/webpack-stats-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/erykpiast/webpack-stats-explorer/issues"
  },
  "homepage": "https://github.com/erykpiast/webpack-stats-explorer",
  "main": "dist/main.js",
  "files": [
    "dist/index.html",
    "dist/main.js"
  ],
  "scripts": {
    "clean": "rm -rf dist && bsb -clean-world",
    "precompile": "npm run clean",
    "compile": "bsb -make-world",
    "prebuild": "npm run compile",
    "build": "npm run build:app",
    "build:app": "NODE_ENV=production webpack",
    "build:playground": "npm run playground:play",
    "start": "bsb -make-world -w",
    "pretest": "npm run compile",
    "test": "npm run test:unit",
    "test:unit": "TEST=true jest",
    "test:unit:watch": "npm run test:unit -- --watch",
    "webpack": "webpack-dev-server",
    "format": "find src -name '*.ml' -exec xargs ocamlformat -i -p janestreet {} \\;",
    "release": "standard-version",
    "playground:play": "./playground/play.sh",
    "playground:upgrade-deps": "./playground/upgrade-deps.sh",
    "prepublish": "npm run build"
  },
  "keywords": [
    "webpack",
    "analyzer",
    "bundle",
    "diff",
    "compare",
    "stats"
  ],
  "author": "Eryk Napiera≈Ça <eryk.piast@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ahrefs/bs-recharts": "0.3.3",
    "@glennsl/bs-json": "5.0.2",
    "@glennsl/rebase": "0.2.1",
    "@zpnester/bs-filereader": "0.4.0",
    "bs-css": "https://gitpkg.now.sh/erykpiast/bs-css/bs-css?width-values",
    "bs-css-emotion": "https://gitpkg.now.sh/erykpiast/bs-css/bs-css-emotion?width-values",
    "bs-fetch": "0.6.1",
    "bs-js-beautify": "0.1.1",
    "bs-jsverify": "0.10.0",
    "bs-react-diff-viewer": "0.1.3",
    "bs-react-dropzone": "0.5.3",
    "bs-react-syntax-highlighter": "0.2.2",
    "bs-webapi": "0.16.1",
    "date-fns": "2.14.0",
    "diff": "4.0.2",
    "js-beautify": "1.11.0",
    "rationale": "0.2.0",
    "re-classnames": "5.0.1",
    "react": "16.13.1",
    "react-diff-viewer": "3.1.1",
    "react-dom": "16.13.1",
    "react-dropzone": "11.0.1",
    "react-syntax-highlighter": "12.2.1",
    "reason-date-fns": "0.10.4",
    "reason-react": "0.9.1",
    "recharts": "1.8.5"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@glennsl/bs-jest": "0.5.1",
    "babel-jest": "25.1.0",
    "bs-enzyme": "0.6.1",
    "bs-platform": "7.3.2",
    "enzyme-adapter-react-16": "1.15.2",
    "gh-pages": "3.0.0",
    "html-webpack-plugin": "4.2.2",
    "jest": "25.1.0",
    "jsverify": "0.8.4",
    "npm-run-all": "4.1.5",
    "standard-version": "8.0.1",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.0"
  }
}
